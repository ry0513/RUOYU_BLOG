    "@vueuse/head": "^2.0.0",
    "axios": "^1.6.3",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "connect-redis": "^7.1.0",
    "dayjs": "^1.11.10",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "fs-extra": "^11.2.0",
    "joi": "^17.11.0",
    "log4js": "^6.9.1",
    "mysql2": "^3.6.5",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "qs": "^6.11.2",
    "redis": "^4.6.12",
    "request-ip": "^3.3.0",
    "sequelize-typescript": "^2.1.6",
    "tdesign-vue-next": "^1.7.2",
    "vue": "^3.3.13",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.3",
    "@types/compression": "^1.7.3",
    "@types/express": "^4.17.18",
    "@types/express-session": "^1.17.8",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.7.1",
    "@types/nprogress": "^0.2.1",
    "@types/qs": "^6.9.11",
    "@types/request-ip": "^0.0.39",
    "@types/uuid": "^9.0.4",
