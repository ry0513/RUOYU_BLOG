{
  "name": "ruoyu_blog",
  "version": "0.3.1-alpha.2",
  "description": "",
  "type": "module",
  "scripts": {
    "build:webClient": "vite build --ssrManifest --emptyOutDir --outDir  ./dist/client",
    "build:webServer": "vite build --ssr web/entry-server.ts --emptyOutDir --outDir ./dist/server",
    "build:serve": "cd serve && tsc",
    "build": "pnpm run build:webClient && pnpm run build:webServer && pnpm run build:serve",
    "dev": "cross-env NODE_ENV=development NODE_PORT=4500 ts-node --files serve/app.ts",
    "dev:watch": "nodemon --watch 'serve/**/*' -e ts,tsx --exec 'pnpm run dev'",
    "start": "node dist/src/app.js",
    "docker:build": "pnpm run build && docker build -t ccr.ccs.tencentyun.com/ry0513/blog:v$npm_package_version .",
    "docker:push": "docker push ccr.ccs.tencentyun.com/ry0513/blog:v$npm_package_version",
    "docker": "pnpm run docker:build && pnpm run docker:push"
  },
  "repository": {
    "type": "git",
    "url": "https://git.changyan.cc/ry0513/RuoYu_Blog.git"
  },
  "author": "",
  "license": "ISC",
